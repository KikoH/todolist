<% @todolists.newest_first.each do |list| %>
<div id="lists">
	<p id="list-title"> <%= list.title %> </p>
	<p id="list-description"> <%= list.description %> </p>
	<p id="list-delete"><%= link_to "Delete", list , method: :delete, data: {confirm: "Are you sure you want to delete this list?"} %></p>
</div>
<% end %>